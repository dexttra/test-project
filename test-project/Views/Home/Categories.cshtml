@model List<CategoryViewModel>


<h1>Список категорий</h1>
<div class="row">
	<div class="row">
		<table class="table">
			<thread>
				<tr>
					<th>ID</th>
					<th>Название</th>
				</tr>
			</thread>
			<tbody>
				@for (int i = 0; i < Model.Count; i++)
				{
					<tr>
						<td>@Model[i].Id</td>
						<td>@Model[i].Name</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>

